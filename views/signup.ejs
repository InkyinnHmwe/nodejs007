<!DOCTYPE html>
<html lang="en">
<head>
	<!-- <% include partial/head %> -->
</head>
<body>
	<div class="wrapper">
		<div class="main-header">
			<!-- Logo Header -->
		<!-- <% include partial/logo-header %> -->
			<!-- End Logo Header -->

			<!-- Navbar Header -->
			<!-- <% include partial/navbar-header %> -->
			<!-- End Navbar -->
		</div>

		<!-- Sidebar -->
	<!-- <% include partial/sidebar %> -->
		<!-- End Sidebar -->

		<div class="main-panel">
			<div class="content">
        <form name="frm" class="" action="/signup" method="post">
          <div class="row">
            <div class="col-md-12">
              <div class="card">
                <div class="card-header">
                  <div class="card-title">Sign Up</div>
                </div>
                <div class="card-body">
                  <div class="row">
                    <div class="col-md-8 col-lg-8">
                      <div class="form-group">
                        <label for="name">Admin Name</label>
                        <input type="text" class="form-control" id="name" name="name" placeholder="Enter Name">
                        <!-- <small id="emailHelp2" class="form-text text-muted">We'll never share your email with anyone else.</small> -->
                      </div>
                      <div class="form-group">
                        <label for="email">Admin Email</label>
                        <input type="email" class="form-control" id="email" name="email" placeholder="Enter Email">
                        <small id="emailHelp2" class="form-text text-muted">We'll never share your email with anyone else.</small>
                      </div>
                      <div class="form-group">
                        <label for="password">Password</label>
                        <input type="password" class="form-control" id="pwd" name="pwd" placeholder="Password">
                      </div>

                    </div>
                  </div>
                </div>
                <div class="card-action">
                  <button type="button" id="sub" name="button" class="btn btn-success">Sign Up</button>
                </div>
            </div>
          </div>
        </form>
      </div>
    </div>
			<!-- <% include partial/footer %> -->
		</div>


	</div>
	<!--   Core JS Files   -->
	<!-- <% include partial/script %> -->
</body>

    <script  src="/javascripts/jquery-3.4.1.js"
    integrity="sha256-WpOohJOqMqqyKL9FccASB9O0KwACQJpFTUBLTYOVvVU="
    crossorigin="anonymous"></script>
    <script type="text/javascript">
        $('#email').blur(function(){
          var data = {email:$('#email').val()};
          if(data.email != ''){
            $.ajax({url:'/duemail', data:data, type:'POST',
            success:function (result){
              if(result.status){
                alert('duplicate email');
                $('#email').val('');
                $('#email').focus();
            }
          }
        })
      }

      })

      $('#sub').click(function(){
        var datap = {password:$('#pwd').val()};
        if(datap.pwd !=''){
          $.ajax({url:'/passvalid', data:datap, type:'POST',
          success:function(result){
            if(result.status){
            if($('#email').val() == '' || $('#name').val() == ''){
              alert('please enter other field')
            }else{
              frm.submit();
            }
          }
            else{
              alert('password invalid');
              $('#pwd').val('');
              $('#pwd').focus();
            }
          }
        })
        }
      })
    </script>

</html>














